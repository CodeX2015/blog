<h1>Welcome</h1>
<h1>Listing articles</h1>
<td><%= link_to 'Create', new_article_path %></td>
<table>
  <tr>
    <th>Title</th>
    <th>Author</th>
    <th>Body</th>
  </tr>

  <% @articles.each do |article| %>
    <tr>
      <td><%= article.title %></td>
      <td><%= article.author %></td>
      <td><%= article.body %></td>
      <td><%= link_to 'Show', article_path(article) %></td>
    </tr>
  <% end %>
</table>